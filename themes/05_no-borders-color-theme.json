  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#98A8C5"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#AD82CB"
      }
    },
    {
      "name": "Escape characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#6DB3CE"
      }
    },
    {
      "name": "Named entities",
      "scope": "entity.name",
      "settings": {
        "foreground": "#FFF9EE"
      }
    },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFD484"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#7EDDDE"
      }
    },
    {
      "name": "Static types",
      "scope": ["entity.name.type", "storage.type.cs"],
      "settings": {
        "foreground": "#8FC8BB"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E78482"
      }
    },
    {
      "name": "CSS class selector",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "fontStyle": "",
        "foreground": "#7EDDDE"
      }
    },
    {
      "name": "CSS transition name",
      "scope": "support.type.property-name.css",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFD484"
      }
    },
    {
      "name": "CSS ID selectors",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFD484"
      }
    },
    {
      "name": "CSS colors",
      "scope": "constant.other.color.rgb-value.hex.css",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFD484"
      }
    },
    {
      "name": "SCSS include & mixins params",
      "scope": ["meta.at-rule.mixin", "meta.at-rule.include"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFD484"
      }
    },
    {
      "name": "SCSS include & mixins parenthesis",
      "scope": [
        "punctuation.definition.parameters.begin.bracket.round",
        "punctuation.definition.parameters.end.bracket.round"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FFF9EE"
      }
    },
    {
      "name": "Inherited classes",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#FFD484"
      }
    },
    {
      "name": "CSS constant",
      "scope": "constant.numeric.css",
      "settings": {
        "foreground": "#8FC8BB"
      }
    },
    {
      "name": "CSS units",
      "scope": "keyword.other.unit",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFF9EE"
      }
    },
    {
      "name": "Pseudo CSS",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#E78482"
      }
    },
    {
      "name": "Variable names",
      "scope": ["entity.name.variable", "variable"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#fff9ee"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#AD82CB"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#6DB3CE"
      }
    },
    {
      "name": "Special operators",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.delete"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#6DB3CE"
      }
    },
    {
      "name": "Markup quotes",
      "scope": "markup.quote",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6DB3CE"
      }
    },
    {
      "name": "Markup headings",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#E78482"
      }
    },
    {
      "name": "Markup bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#AD82CB"
      }
    },
    {
      "name": "Markup italics",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7EDDDE"
      }
    },
    {
      "name": "Markup code",
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#E78482"
      }
    },
    {
      "name": "Markup links",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#FFD484"
      }
    },
    {
      "name": "Storage keywords",
      "scope": "storage",
      "settings": {
        "foreground": "#fff9ee"
      }
    },
    {
      "name": "Strings",
      "scope": ["string.quoted", "string.template"],
      "settings": {
        "foreground": "#98A8C5"
      }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#FFF9EE"
      }
    },
    {
      "name": "Markup link descriptions",
      "scope": "string.other.link",
      "settings": {
        "foreground": "#FFF9EE"
      }
    },
    {
      "name": "Provided (support) values",
      "scope": "support",
      "settings": {
        "foreground": "#8FC8BB"
      }
    },
    {
      "name": "Provided functions",
      "scope": "support.function",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#8FC8BB"
      }
    },
    {
      "name": "Provieded variables",
      "scope": "support.variable",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#8FC8BB"
      }
    },
    {
      "name": "Object/JSON property names",
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": {
        "foreground": "#FFD484"
      }
    },
    {
      "name": "CSS property names",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#fff9ee"
      }
    },
    {
      "name": "Language variables",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFD484"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Embedded template punctuation",
      "scope": "string.template meta.embedded",
      "settings": {
        "foreground": "#fff9ee"
      }
    },
    {
      "name": "Tag brackets",
      "scope": "punctuation.definition.tag",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#98A8C5"
      }
    },
    {
      "name": "Key/value separators",
      "scope": "punctuation.separator",
      "settings": {
        "foreground": "#98A8C5"
      }
    },
    {
      "name": "Template expressions",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#f5f5f5"
      }
    },
    {
