  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#98A8C5"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Escape characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#6DB3CE"
      }
    },
    {
      "name": "Named entities",
      "scope": "entity.name",
      "settings": {
        "foreground": "#FFF9EE"
      }
    },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#7EDDDE"
      }
    },
    {
      "name": "Static types",
      "scope": ["entity.name.type", "storage.type.cs"],
      "settings": {
        "foreground": "#8FC8BB"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
