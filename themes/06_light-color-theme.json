    "editor.lineHighlightBorder": "#e7e7e725",
    "editor.selectionBackground": "#d4d6e74d",
    "editor.selectionHighlightBackground": "#b5b6c54d",
    "editorIndentGuide.background": "#eeeeee",
    "editorLineNumber.foreground": "#cccccc",
    "editorBracketMatch.background": "#f1f8ff",
