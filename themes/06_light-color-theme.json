    "editorGroupHeader.tabsBackground": "#fafbfc",
    "editorGroupHeader.tabsBorder": "#e1e4e8",
    "tab.activeBackground": "#ffffff",
    "tab.activeBorder": "#f0bf65",
    "tab.inactiveBackground": "#fafbfc",
    "tab.border": "#e1e4e8",
    "tab.inactiveForeground": "#586069",
    "scrollbar.shadow": "#00000000",
    "terminal.ansiBlue": "#6DB3CE",
    "terminal.ansiBrightBlue": "#6DB3CE",
    "terminal.ansiBrightCyan": "#7EDDDE",
    "terminal.ansiBrightGreen": "#8FC8BB",
    "terminal.ansiBrightMagenta": "#AD82CB",
    "terminal.ansiBrightRed": "#E78482",
    "terminal.ansiBrightYellow": "#f0bf65",
    "terminal.ansiCyan": "#7EDDDE",
    "terminal.ansiGreen": "#8FC8BB",
    "terminal.ansiMagenta": "#AD82CB",
    "terminal.ansiRed": "#E78482",
    "terminal.ansiYellow": "#f0bf65",
    "gitDecoration.addedResourceForeground": "#8FC8BB",
    "gitDecoration.modifiedResourceForeground": "#f0bf65",
    "gitDecoration.deletedResourceForeground": "#E78482",
    "gitDecoration.untrackedResourceForeground": "#AD82CB",
    "gitDecoration.ignoredResourceForeground": "#14141441"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#98A8C5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Escape characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#6DB3CEc4"
      }
    },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#7EDDDE",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Static types",
      "scope": ["entity.name.type", "storage.type.cs"],
      "settings": {
        "foreground": "#8FC8BB"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Pseudo CSS",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#f0bf65"
      }
    },
    {
      "name": "Variable names",
      "scope": ["entity.name.variable", "variable"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#1A1C23",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Special operators",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.delete"
      ],
      "settings": {
        "foreground": "#1A1C23",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup quotes",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#6DB3CEc4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#f0bf65",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup italics",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#7EDDDE",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string.quoted", "string.template"],
      "settings": {
        "foreground": "#39465E"
      }
    },
    {
      "name": "Provided (support) values",
      "scope": "support",
      "settings": {
        "foreground": "#8FC8BB"
      }
    },
    {
      "name": "Provided functions",
      "scope": "support.function",
      "settings": {
        "foreground": "#8FC8BB",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Provieded variables",
      "scope": "support.variable",
      "settings": {
        "foreground": "#8FC8BB",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Object/JSON property names",
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": {
        "foreground": "#f0bf65"
      }
    },
    {
      "name": "Language variables",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#f0bf65",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag brackets",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#39465E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Key/value separators",
      "scope": "punctuation.separator",
      "settings": {
        "foreground": "#39465E"
      }
    },
    {
      "name": "Template expressions",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#6C6F93"
      }
    },
    {
      "name": "Embedded section punctuation",
      "scope": "punctuation.section.embedded",
      "settings": {
        "fontStyle": "bold"
      }
    }
  ]
}
